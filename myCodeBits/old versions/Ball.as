package myCodeBits{	//import particle;	import flash.events.TimerEvent;	import flash.geom.Point;	import flash.display.MovieClip;		public class Ball extends particle	{		public function Ball($position:Point, $vector:Point, $gravity:Number, $friction:Number)		{			super($position, $vector, $gravity, $friction);						x = position.x;			y = position.y;					update_i.addEventListener(TimerEvent.TIMER, setPosition, false, 0, true);		}				public function setPosition(e:TimerEvent):void		{			x = position.x;			y = position.y;		}	}}